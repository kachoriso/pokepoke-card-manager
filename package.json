{
  "name": "pokepoke-manager-monorepo",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.6.4",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "dev": "concurrently \"pnpm --filter client run dev\" \"pnpm --filter server run start:dev\"",
    "dev:client": "pnpm --filter client dev",
    "dev:server": "pnpm --filter server start:dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "concurrently": "^9.1.2",
    "prettier": "^3.x.x",
    "supabase": "^2.22.4",
    "typescript": "^5.x.x"
  },
  "dependencies": {
    "prisma": "^6.6.0"
  }
}